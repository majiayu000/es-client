{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "es-lif",
  "version": "0.1.0",
  "identifier": "com.es-lif.app",
  "build": {
    "frontendDist": "../src",
     "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "es-lif",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
